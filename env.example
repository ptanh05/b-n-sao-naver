# Environment Variables for Time Management App

# API Configuration
REACT_APP_API_URL=http://localhost:3001/api

# Database Configuration (Neon.tech)
DATABASE_URL=postgres://username:password@ep-xxxx.neon.tech/neondb

# App Configuration
REACT_APP_APP_NAME=Time Management App
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_SUGGESTIONS=true
REACT_APP_ENABLE_SYNC=true

# Notification Settings
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_NOTIFICATION_DURATION=5000

# Sync Settings
REACT_APP_SYNC_INTERVAL=5
REACT_APP_AUTO_SYNC=true
